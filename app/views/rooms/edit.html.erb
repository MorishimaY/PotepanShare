<h3>ユーザー編集</h3>
 
<%= form_with model: @room do |f| %>
  <table>
    <tr>
      <th><%= f.label :name ,"施設名" %></th>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <th><%= f.label :detail ,"施設詳細" %></th>
      <td><%= f.text_field :detail %></td>
    </tr>
    <tr>
      <th><%= f.label :price ,"料金" %></th>
      <td><%= f.text_field :price %></td>
    </tr>
    <tr>
      <th><%= f.label :address ,"住所" %></th>
      <td><%= f.text_area :address %></td>
    </tr>
    <tr>
      <th><%= f.label :room_image ,"施設画像" %></th>
      <td><%= f.text_field :room_image %></td>
    </tr>
  </table>
 
   <div>
     <ul>
       <li><%= f.submit "編集を完了する" %></li>
       <li><%= link_to "登録済み情報一覧に戻る", :rooms_own %></li>
     </ul>
   </div>
 <% end %>